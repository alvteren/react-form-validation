{
  "name": "@feerzlay/react-form-validation",
  "version": "0.3.0",
  "description": "Yet another form validation library for React ಠ_ಠ",
  "main": "out/index.js",
  "scripts": {
    "start": "webpack-serve --config ./example/webpack.config.js --content ./example/out",
    "lint-staged": "lint-staged",
    "transpile": "babel source -d out --minified --copy-files",
    "deploy": "rimraf ./example/out/* && NODE_ENV=production webpack --config ./example/webpack.config.js --progress && gh-pages -d ./example/out",
    "prepublishOnly": "rimraf ./out/* && npm run transpile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feerzlay/react-form-validation.git"
  },
  "keywords": [
    "react",
    "form",
    "validation",
    "validator"
  ],
  "author": "Denis Yakshov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feerzlay/react-form-validation/issues"
  },
  "homepage": "https://github.com/feerzlay/react-form-validation#readme",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.0.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  }
}
